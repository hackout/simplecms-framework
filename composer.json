{
    "name": "simplecms/framework",
    "description": "Simple-CMS is a SimpleCMS",
    "keywords": [
        "laravel",
        "framework",
        "simplecms"
    ],
    "version": "1.0.2",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SimpleCMS\\Framework\\": "src/"
        }
    },
    "authors": [
        {
            "name": "hackout",
            "email": "hackout@vip.qq.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "alibabacloud/dysmsapi-20170525": "^2.0",
        "laravel/framework": "^11.0",
        "laravel/tinker": "^2.9",
        "maatwebsite/excel": "^3.1",
        "spatie/laravel-medialibrary": "^11.5",
        "intervention/image": "^3.6",
        "php-ffmpeg/php-ffmpeg": "^1.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SimpleCMS\\Framework\\SimpleServiceProvider"
            ],
            "aliases": {
                "SystemConfig": "SimpleCMS\\Framework\\Facades\\SystemConfig",
                "SystemInfo": "SimpleCMS\\Framework\\Facades\\SystemInfo",
                "ExcelConvert": "SimpleCMS\\Framework\\Facades\\ExcelConvert",
                "ExcelDrawing": "SimpleCMS\\Framework\\Facades\\ExcelDrawing",
                "Captcha": "SimpleCMS\\Framework\\Facades\\Captcha",
                "Dict": "SimpleCMS\\Framework\\Facades\\Dict",
                "Menu": "SimpleCMS\\Framework\\Facades\\Menu"
            }
        }
    }
}